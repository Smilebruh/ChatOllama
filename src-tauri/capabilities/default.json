{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
 "permissions": [
    "core:app:default",       
    "opener:default",
    "websocket:default",
    "shell:default",       
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/ollama_api",
          "sidecar": true
        }
      ]
    }
  ]
}